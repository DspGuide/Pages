{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude":[
          "**/include/**",
          "**/_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "templates/dsp/dsp"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": true,
    "build": {
      "globalMetadata": {
        "_baseUrl": "https://dspguide.github.io/Pages",
        "_appTitle": "The Unofficial Reddit Guide to Dyson Sphere Program",
        "_appName": "Reddit's DSP Guide",
        "_enableNewTab": true
      },
      "sitemap": {
        "baseUrl": "https://dspguide.github.io/Pages",
        "priority": 0.1,
        "changefreq": "monthly"
      }
    }
  }
}